Print 1stnumber lastnumber 2ndnumber (last-1)number:

#include <stdio.h>
int main() {
    int num;
    scanf("%d", &num);  // Read number first
    int arr[num];  // Declare array *after* reading num
    for (int i = 0; i < num; i++) {
        scanf("%d", &arr[i]);
    }
    int leftIndex = 0, rightIndex = num - 1;
    while (leftIndex < rightIndex) {
        printf("%d %d ", arr[leftIndex], arr[rightIndex]);
        leftIndex++;
        rightIndex--;
    }
    if (num % 2 != 0) {
        printf("%d", arr[leftIndex]);
    }
    return 0;
}
if(num%2!=0)
{
    printf("%d ",arr[leftIndex]);
}
}
